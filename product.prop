# ADB On Boot
ro.adb.secure=0
ro.secure=0
ro.debuggable=1
persist.service.debuggable=1
persist.service.adb.enable=1
persist.sys.usb.config=adb

# Audio
audio.sys.noisy.broadcast.delay=500
audio.sys.mute.latency.factor=2
audio.sys.routing.latency=300
audio.offload.min.duration.secs=30
audio.sys.offload.pstimeout.secs=3

# Blurs (hidden under dev option)
ro.surface_flinger.supports_background_blur=1
persist.sys.sf.disable_blurs=1
ro.sf.blurs_are_expensive=1

# Graphics
debug.hwui.renderer=skiavk
ro.surface_flinger.max_frame_buffer_acquired_buffers=3

# LMKD
ro.config.low_ram=false
ro.lmk.log_stats=true

# VoLTE
persist.dbg.volte_avail_ovr=1
persist.dbg.vt_avail_ovr=1
persist.dbg.wfc_avail_ovr=1

# Shutdown
ro.build.shutdown_timeout=2

# Telephony
ro.telephony.default_network=22,20

# Zygote Preforking
persist.device.config.runtime_native.uasp_pool_enabled=true

# Priv-app permission
PRODUCT_PRODUCT_PROPERTIES += \
	ro.control_privapp_permissions=log

# Color control
PRODUCT_PROPERTY_OVERRIDES += \
    ro.surface_flinger.has_wide_color_display=1 \
    ro.surface_flinger.use_color_management=1
